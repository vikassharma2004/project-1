<%layout("/layouts/boilerplate") %>
</head>
<style>
    body{
    font-family: 'Plus Jakarta Sans', sans-serif !important;
    display: flex;
    min-height: 100vh;
    flex-direction: column;
}
.container{
    flex: 1;
}
.navbar{
    height: 5rem;
}
.fa-compass{
    color: #fe424d;
    font-size: 2rem;
}
.nav-link{
    color: rgb(34, 34, 34) !important;
    
}
@media screen and (max-width: 991px) {
    /* Adjust background color for nav-link on medium-sized devices */
    .nav-link {
       z-index: 1 !important;
    float: right;
 
      background-color: aliceblue;
     
    }
    .nav-link a{
        color: white !important;
    }
  }
  

.f-info-links a{
text-decoration: none;
color: #222222;
}
.f-info-links a:hover{
text-decoration: underline;

}
.f-info{
    text-align: center;
    height: 8rem;
    background-color: #201f1f4e;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items:space-evenly;
}

.f-info-socials i{
    margin-right: 2rem;
    font-size: 1.2rem;
}


.f-info-links,.f-info-socials,.f-info-brand{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
/*  cards */
.card{
    border: none !important;
    margin-bottom: 2rem;
}
.card-img-top{
    border-radius: 1rem !important;
    width: 100% !important;
    object-fit: cover !important;
}
.card-body{
    padding: 0 !important;
}
.card-text {
    font-weight: 400 !important;
}
.listing-link{
    text-decoration: none;
}
.card-img-overlay{
    opacity: 0;
}
.card-img-overlay:hover{
    opacity: 0.2;
    background-color: white;
}
</style>
</head>
<div class="row">
    <div class="alert alert-danger col-6 offset-3 mt-3 " role="alert">
        <p><%=err%></p>
    </div>
</div>